  "name": "admin-panel",
  "version": "0.0.0",
  "engines": {
    "node": "^12.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-path=dist",
    "build:prod": "ng build --prod --output-path=dist",
    "test": "jest",
    "test-watch": "jest --watch",
    "lint": "ng lint",
    "e2e": "cypress open",
    "lint-staged": "lint-staged",
    "eslint": "eslint *.js",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 9008 -s src/assets",
    "build-storybook": "build-storybook",
    "ci:commitlint": "commitlint",
    "ci:clear-test-attributes": "node pipeline/clear-test-attributes.js",
    "ci:e2e": "cypress run --headless --reporter junit --reporter-options mochaFile=e2e/results/output-[hash].xml,toConsole=true",
    "ci:prettier": "prettylint --config ./.prettierrc.js",
    "ci:stylelint": "stylelint '**/*.scss'",
    "ci:tslint": "tslint src/**/*.ts && tslint e2e/**/*.ts",
    "ci:check-translation": "node pipeline/check-translation.js"
  },
  "lint-staged": {
    "*.{scss,ts,tsx,js,json,md,html}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint"
    ]
  },
  "pre-commit": "lint-staged",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
